<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>



    <% results.forEach(function(product){ %>

        <form method="post">
            <table>
                <tr>
                    <td>name</td>
                    <td><input type="text" name="name" value="<%= product.name %>" id=""></td>
                </tr>
                <tr>
                    <td>quantity</td>
                    <td><input type="number" name="quantity" value="<%= product.qty %>" id=""></td>
                </tr>
                <tr>
                    <td>price</td>
                    <td><input type="number" name="price" value="<%= product.price %>" id=""></td>
                </tr>
                <tr>
                    <td></td>
                    <td><input type="submit" name="save" value="save" id=""></td>
                </tr>
            </table>
        </form>
        <% }); %>





</body>

</html>